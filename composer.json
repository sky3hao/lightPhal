{
    "name": "tengyue/infra",
    "description": "Infrastructure",
    "license": "proprietary",
    "require": {
        "php": ">=7.0",
        "tengyue/zeus": "^1.0",
        "monolog/monolog": "^1.23",
        "sky3hao/mns_client": "~1.0",
        "linkorb/etcd-php": "v1.5.0",
        "illuminate/database": "~5.5",
        "illuminate/pagination": "~5.5"
    },
    "authors": [
        {
            "name": "Fulin Liu",
            "email": "liufulin@tengyue360.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.office.tengyue360.com/infra/zeus.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sky3hao/mns_client.git"
        }
    ],
    "config": {
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "Tengyue\\Infra\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "autoload-dev": {
        "psr-4": {
            "InfraTest\\": "test/Cases"
        }
    }
}